<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hc.hyh.system.dao.AchieveMapper">

    <select id="getId_Score" resultType="com.hc.hyh.system.model.Achieve">
        select * from achievement where idno=#{idno} and name=#{name}
    </select>

    <select id="getTest_Score" resultType="com.hc.hyh.system.model.Achieve">
        select * from achievement where testno=#{testno} and name=#{name}
    </select>

</mapper>